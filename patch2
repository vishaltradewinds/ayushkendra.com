diff --git a/scripts/check_deploy_readiness.sh b/scripts/check_deploy_readiness.sh
new file mode 100755
index 0000000000000000000000000000000000000000..90e0353b98267b055bb40fa530e61041b2e64f40
--- /dev/null
+++ b/scripts/check_deploy_readiness.sh
@@ -0,0 +1,32 @@
+#!/usr/bin/env bash
+set -euo pipefail
+
+ROOT_DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")/.." && pwd)"
+cd "$ROOT_DIR"
+
+fail() {
+  echo "[FAIL] $1" >&2
+  exit 1
+}
+
+pass() {
+  echo "[PASS] $1"
+}
+
+[ -f backend/Dockerfile ] || fail "Missing backend/Dockerfile"
+[ -f frontend/Dockerfile ] || fail "Missing frontend/Dockerfile"
+[ -f docker-compose.yml ] || fail "Missing docker-compose.yml"
+[ -f deploy/nginx/default.conf ] || fail "Missing deploy/nginx/default.conf"
+[ -f .env.example ] || fail "Missing .env.example"
+[ -f docs/HOSTINGER_VPS_DEPLOY.md ] || fail "Missing docs/HOSTINGER_VPS_DEPLOY.md"
+
+pass "Required deployment files exist"
+
+if command -v docker >/dev/null 2>&1; then
+  docker compose config >/dev/null
+  pass "docker compose config is valid"
+else
+  echo "[WARN] Docker is not installed in this environment. Skipping compose validation."
+fi
+
+pass "Deploy readiness checks completed"
diff --git a/scripts/deploy_hostinger_vps.sh b/scripts/deploy_hostinger_vps.sh
new file mode 100755
index 0000000000000000000000000000000000000000..2f10775e95242491dc749be7d266f6f61b870963
--- /dev/null
+++ b/scripts/deploy_hostinger_vps.sh
@@ -0,0 +1,25 @@
+#!/usr/bin/env bash
+set -euo pipefail
+
+DOMAIN="${1:-}"
+EMAIL="${2:-}"
+
+if [[ -z "$DOMAIN" || -z "$EMAIL" ]]; then
+  echo "Usage: $0 <domain> <email>"
+  exit 1
+fi
+
+if [[ ! -f .env ]]; then
+  cp .env.example .env
+  echo "Created .env from .env.example (please edit if needed)"
+fi
+
+sudo apt-get update
+sudo apt-get install -y docker.io docker-compose-plugin certbot
+sudo systemctl enable --now docker
+
+docker compose up -d --build
+
+sudo certbot certonly --webroot -w "$(pwd)/deploy/certbot/www" -d "$DOMAIN" --email "$EMAIL" --agree-tos --non-interactive
+
+echo "Deployment completed. Update deploy/nginx/default.conf for TLS server block and reload nginx container."
